{
  "name": "h-extract",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":["*://*.mozilla.org/*"],
      "js": ["borderify.js"]
    }
  ],

  "permissions":[
    "tabs","activeTab"
  ],

  "browser_action":{
    "default_icon": {
      "48": "Fingerprinters.svg"
    },
  "default_title": "Whereami?"
  }
}
